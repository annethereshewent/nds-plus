{
  "compilerOptions": {
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "ES2020",
    "moduleResolution": "node",
    "target": "ES6",
    "sourceMap": true,
    "lib": [
      "ES6",
      "WebWorker",
      "DOM",
      "DOM.Iterable"
    ],
  }
}